                Exit Select
            Case 80
                name = "tám mươi"
                Exit Select
            Case 90
                name = "chín mươi"
                Exit Select
            Case Else
                If digt &gt; 0 Then
                    name = tens(digit.Substring(0, 1) + "0") + " " + ones(digit.Substring(1))
                End If
                Exit Select
        End Select
        Return name
    End Function
    Private Function ones(digit As String) As String
        Dim digt As Integer = Convert.ToInt32(digit)
        Dim name As String = ""
        Select Case digt
            Case 1
